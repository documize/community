<style>
    .github-board {
      width: 100%;
      padding: 10px;
      white-space: nowrap;
    }

    ul.github-list {
      margin: 20px;
      padding: 0;
    }

    .github-group-title {
      margin-bottom: 10px;
      font-size: 14px;
      color: #767676;
    }

    li.github-commit-item {
      display: block;
      width: 95%;
      font-size: 12px;
      color: #999;
      padding: 10px;
      border-left: 1px solid #eee;
      border-right: 1px solid #eee;
      border-top: 1px solid #eee;
    }

    li.github-commit-item:first-of-type {
      border-top-left-radius: 2px;
      border-top-right-radius: 2px;
    }

    li.github-commit-item:last-of-type {
      border-bottom-left-radius: 2px;
      border-bottom-right-radius: 2px;
      border-bottom: 1px solid #eee;
    }

    li.github-commit-item:hover {
      background-color: #f7fbfc;
      cursor: pointer;
    }

    .github-avatar {
      float: left;
      margin-right: 15px;
      margin-top: 2px;
    }

    .github-commit-body {
      margin: 0px;
      padding: 0px;
    }

    .github-commit-title {
      color: #4e575b;
      text-decoration: none;
      font-size: 13px;
      font-weight: 600;
      line-height: 16px;    
      margin: 0px;
      padding: 0px;
      overflow: hidden;
      text-overflow: ellipsis;
      width: 90%; 
    }
   
    .github-commit-meta {
      font-weight: normal;
      color: #767676;
      font-size: 12px;
      line-height: 22px;
      margin: 0px;
      padding: 0px;
      width: 90%; 
    }
</style>

{{{page.body}}}
